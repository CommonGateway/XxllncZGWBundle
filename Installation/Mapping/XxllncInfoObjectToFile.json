{
    "title": "XxllncInfoObjectToFile",
    "$id": "https://development.zaaksysteem.nl/mapping/xxllnc.XxllncInfoObjectToFile.mapping.json",
    "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
    "version": "0.0.1",
    "passTrough": false,
    "mapping": {
        "type": "metadata",
        "naam": "infoObject.titel",
        "metadata.type": "metadata",
        "metadata.instance.appearance": "infoObject.informatieobject.bestandsnaam",
        "metadata.instance.description": "infoObject.informatieobject.beschrijving",
        "metadata.instance.origin": "Inkomend",
        "metadata.instance.origin_date": "infoObject.informatieobject.creatiedatum",
        "metadata.instance.pronom_format": "infoObject.informatieobject.formaat",
        "metadata.instance.structure": "text",
        "metadata.instance.trust_level": "{% if infoObject.integriteit.waarde|default %}{{ infoObject.integriteit.waarde }}{% else %}Openbaar{% endif %}",
        "metadata.instance.status": "original",
        "metadata.instance.creation_date": "infoObject.informatieobject.creatiedatum"
    }
}