{
    "title": "XxllncValueToZaakEigenschap",
    "$id": "https://development.zaaksysteem.nl/mapping/xxllnc.XxllncValueToZaakEigenschap.mapping.json",
    "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
    "version": "0.0.1",
    "passTrough": false,
    "mapping": {
        "naam": "{{ key }}",
        "waarde": "{{ value|first }}",
        "eigenschap": "{% for eigenschap in eigenschappen %}{% if key == eigenschap.naam %}{{ eigenschap['_self']['id'] }}{% endif %}{% endfor %}"
    }
  }
